{
    "extends": [
        "eslint-config-kyt"
    ],

    "env": {
    },

    "plugins": [
    ],

    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "class-methods-use-this": [0],
        "no-underscode-dangle": ["error", { "allowAfterThis": true }],
        "constructor-super": "error"
    }
}
